            "fontFamily": "{fontFamily.brand}",
            "fontWeight": "{fontWeight.bold}",
            "fontSize": "{fontSize.display.lg}",
            "lineHeight": "{lineHeight.md}",
            "letterSpacing": "{letterSpacing.tight}"
          }
        },
        "md": {
          "$type": "typography",
          "$value": {
            "fontFamily": "{fontFamily.brand}",
            "fontWeight": "{fontWeight.bold}",
            "fontSize": "{fontSize.display.md}",
